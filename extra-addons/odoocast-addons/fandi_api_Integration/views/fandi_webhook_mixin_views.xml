<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- View fandi.webhook.mixin View Tree -->
    <record id="view_fandi_webhook_mixin_tree" model="ir.ui.view">
        <field name="name">view.fandi.webhook.mixin.tree</field>
        <field name="model">fandi.webhook.mixin</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <!-- View fandi.webhook.mixin form -->
    <record id="view_fandi_webhook_mixin_form" model="ir.ui.view">
        <field name="name">view.fandi.webhook.mixin.form</field>
        <field name="model">fandi.webhook.mixin</field>
        <field name="arch" type="xml">
            <form string="Fandi Webhook Mixin">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- View fandi.webhook.mixin search -->
    <record id="view_fandi_webhook_mixin_search" model="ir.ui.view">
        <field name="name">view.fandi.webhook.mixin.search</field>
        <field name="model">fandi.webhook.mixin</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action fandi.webhook.mixin -->
    <record id="action_fandi_webhook_mixin" model="ir.actions.act_window">
        <field name="name">Fandi Webhook Mixin</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">fandi.webhook.mixin</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new Fandi Webhook Mixin.
            </p>
        </field>
    </record>

</odoo>
