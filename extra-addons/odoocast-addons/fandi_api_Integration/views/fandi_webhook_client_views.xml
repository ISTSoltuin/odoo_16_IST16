<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- View fandi.webhook.client View Tree -->
    <record id="view_fandi_webhook_client_tree" model="ir.ui.view">
        <field name="name">view.fandi.webhook.client.tree</field>
        <field name="model">fandi.webhook.client</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <!-- View fandi.webhook.client form -->
    <record id="view_fandi_webhook_client_form" model="ir.ui.view">
        <field name="name">view.fandi.webhook.client.form</field>
        <field name="model">fandi.webhook.client</field>
        <field name="arch" type="xml">
            <form string="Fandi Webhook Client">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- View fandi.webhook.client search -->
    <record id="view_fandi_webhook_client_search" model="ir.ui.view">
        <field name="name">view.fandi.webhook.client.search</field>
        <field name="model">fandi.webhook.client</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action fandi.webhook.client -->
    <record id="action_fandi_webhook_client" model="ir.actions.act_window">
        <field name="name">Fandi Webhook Client</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">fandi.webhook.client</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new Fandi Webhook Client.
            </p>
        </field>
    </record>

</odoo>
