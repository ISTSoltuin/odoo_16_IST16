<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="ren_quotation_tree_view" model="ir.ui.view">
        <field name="name">ren.quotation.tree.view</field>
        <field name="model">ren.quotation</field>
        <field name="arch" type="xml">
            <tree>
                <field name="product_id"/>
                <field name="description"/>
                <field name="price"/>
                <field name="company_id" invisible="1"/>
                <field name="currency_id" invisible="1"/>
            </tree>
        </field>
    </record>
    <record id="action_ren_quotation" model="ir.actions.act_window">
        <field name="name">REN Quotation</field>
        <field name="res_model">ren.quotation</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>
