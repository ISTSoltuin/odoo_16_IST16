<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="ren_bom_tree_view" model="ir.ui.view">
        <field name="name">ren.bom.tree.view</field>
        <field name="model">ren.bom</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="product_id" />
                <field name="product_qty"/>
            </tree>
        </field>
    </record>
    <record id="action_ren_bom" model="ir.actions.act_window">
        <field name="name">Material of Project</field>
        <field name="res_model">ren.bom</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>
