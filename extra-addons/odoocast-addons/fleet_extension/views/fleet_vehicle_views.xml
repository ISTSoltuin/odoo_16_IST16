<odoo>
    <record id="view_fleet_vehicle_form_inherit" model="ir.ui.view">
        <field name="name">fleet.vehicle.form.inherit</field>
        <field name="model">fleet.vehicle</field>
        <field name="inherit_id" ref="fleet.fleet_vehicle_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='location']" position="after">
                <field name="available_for_sale" string="DisponÃ­vel para venda"/>
                <field name="sale_value" widget="monetary"/>
                <field name="maintenance_cost" widget="monetary"/>
                <field name="documentation_cost" widget="monetary"/>
            </xpath>
            <xpath expr="//field[@name='next_assignation_date']" position="after">
                <field name="id_catalogo" />
                <field name="zero_km" />
                <field name="codigo_parceiro" />
                <field name="chassi" />
            </xpath>
            <xpath expr="//field[@name='model_year']" position="after">
                <field name="manufactured_year" />
            </xpath>
        </field>
    </record>
</odoo>
