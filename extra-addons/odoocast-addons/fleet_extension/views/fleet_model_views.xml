<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_fleet_vehicle_model_form" model="ir.ui.view">
        <field name="name">fleet.vehicle.model.form</field>
        <field name="model">fleet.vehicle.model</field>
        <field name="arch" type="xml">
            <form string="Modelo de Veículo">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="brand_id"/>
                        <field name="tipo"/>
                        <field name="porte"/>
                        <field name="molicar"/>
                        <field name="fipe"/>
                        <field name="marca"/>
                        <field name="familia"/>
                        <field name="modelo"/>
                        <field name="combustivel"/>
                        <field name="cavalos"/>
                        <field name="postas"/>
                        <field name="inicio"/>
                        <field name="fim"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_fleet_vehicle_model_tree" model="ir.ui.view">
        <field name="name">fleet.vehicle.model.tree</field>
        <field name="model">fleet.vehicle.model</field>
        <field name="arch" type="xml">
            <tree string="Modelos de Veículos">
                <field name="name"/>
                <field name="brand_id"/>
                <field name="tipo"/>
                <field name="porte"/>
                <field name="molicar"/>
                <field name="fipe"/>
                <field name="marca"/>
                <field name="familia"/>
                <field name="modelo"/>
                <field name="combustivel"/>
                <field name="cavalos"/>
                <field name="postas"/>
                <field name="inicio"/>
                <field name="fim"/>
            </tree>
        </field>
    </record>
</odoo>
